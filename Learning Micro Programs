prices = [10, 20, 30]
sum = 0
for price in prices:
    sum += price
print(sum)

for x in range(1):
    for y in range(1):
        print(f"{x},{y}")


i = ("x")
for a in range(1):
    for b in range(2):
        print(i*5)
        print(i*2)
    print(i*2)

num = [5,2,5,2,2]
for n in num:
    output = ""
    for count in range(n):
        output.append("x")


num = [2,2,2,2,7]
for n in num:
    output = ""
    for count in range(n):
        output += "x"
    print(output)

number = [10,11,12,13,14,24,456,67,78,89,35,45,436]
count = len(number)
max = number[0]
for num in number:
    if num > max:
        max = num
print(f"biggest number is {max}")

number = [1,2,3,4,2,3,5,6]
unique = []
for num in number:
    if num not in unique:
        unique.append(num)
print(unique)

Data = {"1":"One ", "2":"Two ", "3":"Three ", "4":"Four ", "5":"Five ", "6":"Six ", "7":"Seven ", "8":"Eight ", "9":"Nine "}
phone = input("Phone: ")
phone_name = ""
for num in phone:
    phone_name += (Data.get(num, "!")) + " "
print(phone_name)


def emoji(message):
    words = message.split(" ")
    emojis = {":)": "ğŸ™‚", ":(":"ğŸ™"}
    output = ""
    for word in words:
        output += emojis.get(word, word) + " "
    return output
